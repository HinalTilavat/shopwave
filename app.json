{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "shopewave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6464ff55-dd8e-406c-991a-2c0009a64349"
      }
    },
    "android": {
      "package": "com.neophile.boltexponativewind"
    },
    "owner": "neophile"
  }
}
